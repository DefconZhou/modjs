
module.exports = {
    tasks: { 
        cat: {
			options: {
				foo: 1
			},
            sample: {
				options: {
					bar: 2
				},
                source: ['{{ qux }}', 'baz/*.js'],  // [['foo/*.js', 'bar/*.js'], 'baz/*.js']
                dest: 'build/{{ baz }}.js',         // 'build/abcde.js'
            },
        },
        // Arbitrary properties used in task configuration templates.
        foo: 'c',
        bar: 'b{{ foo }}d', // 'bcd'
        baz: 'a{{ bar }}e', // 'abcde'
        qux: ['foo/*.js', 'bar/*.js']
    }
};